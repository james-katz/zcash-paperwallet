{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "cra-template": "1.2.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "preinstall": "curl https://rustwasm.github.io/wasm-pack/installer/init.sh -sSf | sh",
    "start": "react-scripts start",
    "build:wasm": "rm -rf src/wasm && cd .. && wasm-pack build --target web --release --out-dir ./react-app/src/wasm",
    "build": "npm run build:wasm && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "wasm-loader": "^1.3.0",
    "wasm-pack": "^0.12.0"
  }
}
